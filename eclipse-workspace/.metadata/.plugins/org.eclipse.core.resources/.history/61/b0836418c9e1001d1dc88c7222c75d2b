package com.learning;

public class Looping {

	public static void main(String[] args) {
	Looping ll = new Looping();
	ll.print_11111();
//	ll.addition_of_n_number();
}

	private void print_11111() {
		// TODO Auto-generated method stub
		int box = 0;
		int no = 1;
		while(no<=5) {
		box = box + no;
		no = no + 1;}
		System.out.print(1 + " ");
	}
	
}
