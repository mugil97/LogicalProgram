package Arrayprograms;

public class Pattern2 {

	public static void main(String[] args) {
		
		for(int no = 5; no>=1; no--)
	    {
	        for(int star = 1; star<no; star++)
	        {  
	        	System.out.print("* ");     
	        }
	        for (int num = 1; num<=5;num++)
	        {
	        System.out.println(1 + " "); 
	        ; 
	        }
	        System.out.println(); 
	    }
	    
	}

}
